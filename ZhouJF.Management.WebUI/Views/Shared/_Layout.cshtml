@using SmartFast.Web.BaseLibrary;

<!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8 no-js"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9 no-js"> <![endif]-->
<!--[if !IE]><!-->
<html lang="zh-cn" class="no-js">
<!--<![endif]-->
<!-- BEGIN HEAD -->
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title  - 养护平台管理系统</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta content="width=device-width, initial-scale=1.0" name="viewport" />
    <meta http-equiv="Content-type" content="text/html; charset=utf-8" />
    <meta content="" name="description" />
    <meta content="" name="author" />
    <link href="~/assets/global/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/assets/global/plugins/simple-line-icons/simple-line-icons.min.css" rel="stylesheet" />
    <link href="~/assets/global/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/assets/global/plugins/uniform/css/uniform.default.css" rel="stylesheet" />
    <link href="~/assets/global/plugins/bootstrap-switch/css/bootstrap-switch.min.css" rel="stylesheet" />
    <link href="~/Assets/global/Plugins/select2/select2.css" rel="stylesheet" />
    <link href="~/assets/global/plugins/jstree/dist/themes/default/style.min.css" rel="stylesheet" />
    <link href="~/assets/global/css/components.css" rel="stylesheet" />
    <link href="~/assets/global/css/plugins.css" rel="stylesheet" />
    <link href="~/assets/admin/layout/css/layout.css" rel="stylesheet" />
    <link href="~/assets/admin/layout/css/themes/default.css" rel="stylesheet" />
    <link href="~/assets/admin/layout/css/custom.css" rel="stylesheet" />
    <link href="~/Assets/table/table.css" rel="stylesheet" />

    @RenderSection("header", false)
</head>
<body class="page-header-fixed">
    <div class="page-header navbar navbar-fixed-top">
        <div class="page-header-inner">
            <div class="page-logo ">
                <a href="/">
                    <span class="text-logo">养护平台管理系统</span>
                </a>
                <div class="menu-toggler sidebar-toggler hide">

                </div>
            </div>
            <a href="javascript:;" class="menu-toggler responsive-toggler" data-toggle="collapse" data-target=".navbar-collapse"></a>
            @Html.Action("_TopNavMenu", "Shared")
        </div>
    </div>
    <div class="clearfix"></div>
    <div class="page-container">
        @Html.Action("_LeftNavMenu", "Shared")
        <div class="page-content-wrapper">
            <div class="page-content" style="min-height: 572px;">
                @RenderBody()
            </div>
        </div>
    </div>
    <div class="page-footer">
        <div class="page-footer-inner">
            2017 &copy;  YHPT Admin
        </div>
        <div class="scroll-to-top">
            <i class="icon-arrow-up"></i>
        </div>
    </div>
    <div tabindex="-1" class="modal fade in" id="contentLoading" role="basic" aria-hidden="false" style="display: block;">
        <img class="loading" alt="" src="~/Image/ajax-modal-loading.gif">
    </div>
    <div class="modal-backdrop fade in"></div>

    <!-- BEGIN JAVASCRIPTS -->
    @*<script src="~/assets/global/plugins/jquery-1.10.1.min.js"></script>*@
    <script src="~/Assets/global/Plugins/jquery-1.11.0.min.js"></script>
    <script src="~/assets/global/plugins/jquery-migrate-1.2.1.min.js"></script>
    <script src="~/assets/global/plugins/jquery-ui/jquery-ui-1.10.3.custom.min.js"></script>
    <script src="~/assets/global/plugins/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/assets/global/plugins/bootstrap-hover-dropdown/bootstrap-hover-dropdown.min.js"></script>
    <script src="~/assets/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js"></script>
    <script src="~/assets/global/plugins/jquery.blockui.min.js"></script>
    <script src="~/assets/global/plugins/jquery.cookie.min.js"></script>
    <script src="~/assets/global/plugins/uniform/jquery.uniform.min.js"></script>
    <script src="~/assets/global/plugins/bootstrap-switch/js/bootstrap-switch.min.js"></script>
    <script src="~/Assets/global/Plugins/select2/select2.min.js"></script>
    <script src="~/assets/global/scripts/metronic.js"></script>
    <script src="~/assets/admin/layout/scripts/layout.js"></script>
    <script src="~/Assets/global/Plugins/bootstrap-daterangepicker/moment.min.js"></script>
    <script src="~/Assets/global/Plugins/bootBox/bootbox.min.js"></script>
    <link href="~/Assets/global/Plugins/datatables/plugins/bootstrap/dataTables.bootstrap.css" rel="stylesheet" />
    <script src="~/Assets/global/Plugins/datatables/media/js/jquery.dataTables.min.js"></script>
    <script src="~/Assets/global/Plugins/datatables/extensions/TableTools/js/dataTables.tableTools.min.js"></script>
    <link href="~/Assets/global/Plugins/datatables/extensions/FixedColumns/css/dataTables.fixedColumns.min.css" rel="stylesheet" />
    <script src="~/Assets/global/Plugins/datatables/extensions/FixedColumns/js/dataTables.fixedColumns.js"></script>
    <script src="~/Assets/global/Plugins/datatables/plugins/bootstrap/dataTables.bootstrap.js"></script>
    <script src="~/Assets/global/Scripts/dataTableExtention.js"></script>
    <script src="~/Assets/global/Plugins/jquery-bbq-1.2.1/jquery.ba-bbq.min.js"></script>
    <script src="~/Assets/global/Plugins/jquery-validation/jquery.validate.min.js"></script>
    <script src="~/Assets/global/Plugins/My97DatePicker/WdatePicker.js"></script>
    @RenderSection("scripts", false)
    <script>
        jQuery(document).ready(function () {
            Metronic.init();
            Layout.init();
            if (window["pageReady"]) {
                window["pageReady"]();
            }
            $("#contentLoading,.modal-backdrop").remove();
            $(".sidebar-toggler").click(function () {
                var id = $("table.dataTable[id]").attr("id");
                $("#" + id).dataTable().fnDraw(false);
            });
            $(".disabledCutCopyPaste").bind("cut copy paste", function (e) {
                e.preventDefault();
            });
            $('select').addClass("form-control");
        });
    </script>
</body>
</html>
