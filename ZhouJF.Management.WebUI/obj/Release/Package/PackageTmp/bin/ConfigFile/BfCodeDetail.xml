    <Sql Name="GetBatCodeDtls">
      <SqlString>
        <String>
          <![CDATA[
SELECT CD.ID,
       CD.BF_CODE_ID,
       (C.CODE || ' ' || CD.CODE) AS CODE,
       CD.NAME,
       CD.SEQ_NUM,
       CD.LAST_MODIFIED_DATE
  FROM BF_CODE_DETAIL CD
  LEFT JOIN BF_CODE C ON CD.BF_CODE_ID = C.ID
 WHERE C.CODE IN (SELECT BP.CODE
                    FROM SF_BAT_PARS BP
                   WHERE BP.IS_VISIBLE = 1
                     AND BP.IS_INPUT = 0);
]]>
        </String>
      </SqlString>
    </Sql>
    
